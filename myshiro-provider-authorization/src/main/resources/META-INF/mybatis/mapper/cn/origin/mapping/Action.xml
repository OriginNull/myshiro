<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 设置命名空间，可以与不同表的同类型操作进行区分，使用时以“空间名称.id”的方式调用 -->
<mapper namespace="cn.origin.provider.dao.IActionDAO">
	<select id="findAllByMember" parameterType="string" resultType="String">
		SELECT actid FROM action WHERE rid IN (SELECT rid FROM member_role WHERE mid=#{mid}) ;
	</select>
</mapper> 